<?php
include ($_SERVER['DOCUMENT_ROOT'].'/global.php');
$CanSee2020 = $usr['CanSee2020'];
if($CanSee2020 == 1){
$gamequery = $con->prepare("SELECT id,icon,video,name,creatorid,description,MaxPlayers,version,active,banned,created,updated,visits,thumbnail1,thumbnail2,thumbnail3 FROM `games` WHERE `id` = :id");
}else{
$gamequery = $con->prepare("SELECT id,icon,video,name,creatorid,description,MaxPlayers,version,active,banned,created,updated,visits,thumbnail1,thumbnail2,thumbnail3 FROM `games` WHERE `id` = :id AND `version` != 2020");
}
$gamequery->bindValue(':id', $getid, PDO::PARAM_INT);
$gamequery->execute();
$game = $gamequery->fetch(PDO::FETCH_ASSOC);
if(isset($game['id'])) {
$uid = $usr['id'];
$geticon = $game['icon'];
$getthumb1 = $game['thumbnail1'];
$getthumb2 = $game['thumbnail2'];
$getthumb3 = $game['thumbnail3'];
$video = $game['video'];
$gamename = $game['name'];
$gamecreatorid = $game['creatorid'];
$description = $game['description'];
$maxplayers = $game['MaxPlayers'];
$version = $game['version'];
$visits = $game['visits'];
$active = $game['active'];
$cuserquery = $con->prepare("SELECT username FROM `users` WHERE `id` = :id");
$cuserquery->bindValue(':id', $gamecreatorid, PDO::PARAM_INT);
$cuserquery->execute();
$userstuff = $cuserquery->fetch(PDO::FETCH_ASSOC);
$gamecreator = $userstuff['username'];
$countfavequery = $con->prepare("SELECT count(*) FROM `faves` WHERE `assetId` = :assetID");
$countfavequery->execute(['assetID' => $getid]);
$favoritecount = $countfavequery->fetchColumn();
$playercount = getPlayerCount($getid);
$locale = getLocale(false,$getid);
?>
<!DOCTYPE html>
