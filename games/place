Roblox = Roblox || {};
Roblox.BCUpsellModal = function() {
var resources = {
title: "Builders Club Only",
body: "This is a premium item only available to our Builders Club members.",
accept: "Upgrade Now",
decline: "Cancel"
};
var open = function() {
var options = {
titleText: resources.title,
bodyContent: resources.body,
footerText: "",
acceptText: resources.accept,
declineText: resources.decline,
acceptColor: Roblox.GenericConfirmation.green,
onAccept: function() {
window.location.href = "https://www.voidrev.us/premium/membership?ctx=bc-only-game"
},
imageUrl: 'https://www.voidrev.us/img/43ac54175f3f3cd403536fedd9170c10.png'
};
Roblox.GenericConfirmation.open(options);
};
return {
open: open,
Resources: resources
};
}();
